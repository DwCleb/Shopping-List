{
  "name": "shopping-list",
  "version": "1.0.0",
  "description": "Shopping Lits descktop app",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=icons/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --icon=icons/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"ShoppingList\"",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=icons/icon.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DwCleb/Shopping-List.git"
  },
  "author": "Cleber jr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DwCleb/Shopping-List/issues"
  },
  "homepage": "https://github.com/DwCleb/Shopping-List#readme",
  "dependencies": {
    "electron": "^2.0.6"
  },
  "devDependencies": {
    "electron-packager": "^12.1.0"
  }
}